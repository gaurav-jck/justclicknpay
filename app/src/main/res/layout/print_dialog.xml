<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/listItemBackground">


    <LinearLayout
        android:id="@+id/dialogLinear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/transactionDetail"
            android:gravity="center"
            android:background="@color/app_blue_color"
            android:textColor="@color/color_white"
            android:textStyle="bold"
            android:textSize="@dimen/simple_content_text_size"
            android:padding="@dimen/small_margin"/>

        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/small_margin"
            android:paddingBottom="@dimen/small_margin"
            android:background="@drawable/money_print_border_rect_corner"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/medium_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Reservation ID"/>

                <TextView
                    android:id="@+id/reservationID_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.2"
                    android:gravity="right"
                    android:paddingRight="@dimen/small_margin"
                    android:text="12ab00"
                    android:textColor="@color/dark_blue_color"
                    android:textSize="@dimen/text_size_14sp"
                    />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Account No."/>
                <TextView
                    android:id="@+id/acount_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="1111111"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="IFSC Code"/>
                <TextView
                    android:id="@+id/ifsc_Code_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="1230"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Beneficiary Name"/>
                <TextView
                    android:id="@+id/beneficiaryName_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="priyanshi"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Sender Name"/>
                <TextView
                    android:id="@+id/senderName_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="priyanshi"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Sender Mobile No"/>
                <TextView
                    android:id="@+id/senderMobileNo_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Date and Time"/>
                <TextView
                    android:id="@+id/dateTimeTransaction_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Transaction Type"/>
                <TextView
                    android:id="@+id/transactionType_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Txn Amt"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp" />

                <TextView
                    android:id="@+id/txnAmt_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="RBL TransactionID"/>
                <TextView
                    android:id="@+id/transactionID_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Bankrefno "/>
                <TextView
                    android:id="@+id/bankrefno_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="BC Agent Name"/>
                <TextView
                    android:id="@+id/bCAgentName_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/large_margin"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/small_margin"
        android:paddingRight="@dimen/medium_padding">

        <TextView
            android:id="@+id/print_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/small_margin"
            android:layout_marginRight="@dimen/small_margin"
            android:background="@drawable/blue_round_button_background"
            android:gravity="center"
            android:paddingBottom="@dimen/small_margin"
            android:paddingLeft="@dimen/large_margin"
            android:paddingRight="@dimen/large_margin"
            android:paddingTop="@dimen/small_margin"
            android:text=" Print "
            android:textColor="@color/color_white"
            android:textSize="@dimen/simple_content_text_size"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/delete_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/medium_margin"
            android:layout_marginRight="@dimen/small_margin"
            android:background="@color/app_blue_color"
            android:gravity="center"
            android:paddingBottom="@dimen/very_small_margin"
            android:paddingLeft="@dimen/large_margin"
            android:paddingRight="@dimen/large_margin"
            android:paddingTop="@dimen/very_small_margin"
            android:text="Resend Delete OTP"
            android:textColor="@color/color_white"
            android:visibility="gone" />


    </LinearLayout>


</LinearLayout>