<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <RelativeLayout
        android:id="@+id/dialogLinear"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/buttonsLin">
        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MATM Receipt"
            android:gravity="center"
            android:background="@color/app_blue_color"
            android:textColor="@color/color_white"
            android:textStyle="bold"
            android:textSize="@dimen/simple_content_text_size"
            android:padding="@dimen/small_margin"/>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/title"
            android:src="@drawable/app_icon"
            android:alpha=".1"/>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/title">
            <LinearLayout
                android:id="@+id/contentLin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/large_margin"
                android:paddingBottom="@dimen/small_margin"
                android:orientation="vertical"
                android:layout_below="@+id/title"
                android:visibility="visible">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Agent Code"/>
                    <TextView
                        android:id="@+id/agentCodeTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Bank Name"/>
                    <TextView
                        android:id="@+id/bankNameTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:id="@+id/accNoLin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/accountLabelTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Account No"/>
                    <TextView
                        android:id="@+id/accountNoTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />
                <LinearLayout
                    android:id="@+id/mobileNoLin"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/mobileLabelTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Mobile No"/>
                    <TextView
                        android:id="@+id/mobileNoTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="API Txn ID"/>
                    <TextView
                        android:id="@+id/apiTxnIdTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="JCK Txn ID"/>
                    <TextView
                        android:id="@+id/jckTxnIdTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Bank RRN"/>
                    <TextView
                        android:id="@+id/bankRefNoTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Txn Amount"/>
                    <TextView
                        android:id="@+id/remitAmountTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Available Balance"/>
                    <TextView
                        android:id="@+id/availBalTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/textView5"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Txn Type"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp" />

                    <TextView
                        android:id="@+id/txnTypeTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Txn status"/>

                    <TextView
                        android:id="@+id/txnStatusTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1.4"
                        android:gravity="right"
                        android:paddingRight="@dimen/small_margin"
                        android:text=""
                        android:textColor="@color/dark_blue_color"
                        android:textSize="@dimen/text_size_14sp" />

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Txn message"/>
                    <TextView
                        android:id="@+id/benIdTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="100100"
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/small_margin"
                    android:layout_marginLeft="@dimen/small_margin"
                    android:layout_marginRight="@dimen/small_margin"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textColor="@color/printDialogLableColor"
                        android:textSize="@dimen/text_size_14sp"
                        android:text="Time Stamp"/>
                    <TextView
                        android:id="@+id/txnDateTv"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:gravity="right"
                        android:textSize="@dimen/text_size_14sp"
                        android:textColor="@color/dark_blue_color"
                        android:paddingRight="@dimen/small_margin"
                        android:layout_weight="1.2"/>

                </LinearLayout>
                <View
                    android:layout_width="match_parent"
                    android:layout_height="0.5dp"
                    android:background="@color/color_light_black"
                    />

            </LinearLayout>
        </ScrollView>

    </RelativeLayout>

    <TextView
        android:id="@+id/errorTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Transaction Incomplete"
        android:layout_centerInParent="true"
        android:textSize="@dimen/simple_content_text_size"
        android:gravity="center"
        android:textColor="@color/black"
        android:visibility="gone"
        />

    <LinearLayout
        android:id="@+id/buttonsLin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/small_margin"
        android:paddingRight="@dimen/medium_padding"
        android:layout_alignParentBottom="true"
        >

        <TextView
            android:id="@+id/back_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/small_margin"
            android:layout_marginRight="@dimen/small_margin"
            android:background="@drawable/blue_round_button_background"
            android:gravity="center"
            android:paddingBottom="@dimen/small_margin"
            android:paddingLeft="@dimen/large_margin"
            android:paddingRight="@dimen/large_margin"
            android:paddingTop="@dimen/small_margin"
            android:text="Back"
            android:visibility="visible"
            android:textColor="@color/color_white"
            android:textSize="@dimen/simple_content_text_size"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/delete_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/medium_margin"
            android:layout_marginRight="@dimen/small_margin"
            android:background="@color/app_blue_color"
            android:gravity="center"
            android:paddingBottom="@dimen/very_small_margin"
            android:paddingLeft="@dimen/large_margin"
            android:paddingRight="@dimen/large_margin"
            android:paddingTop="@dimen/very_small_margin"
            android:text="Resend Delete OTP"
            android:textColor="@color/color_white"
            android:visibility="gone" />


    </LinearLayout>

</RelativeLayout>