<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <RelativeLayout
        android:id="@+id/dialogLinear"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Receipt"
            android:gravity="center"
            android:background="@color/app_blue_color"
            android:textColor="@color/color_white"
            android:textStyle="bold"
            android:textSize="@dimen/simple_content_text_size"
            android:padding="@dimen/small_margin"/>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/title"
            android:src="@drawable/app_icon"
            android:alpha=".1"/>

        <LinearLayout
            android:id="@+id/contentLin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/small_margin"
            android:paddingBottom="@dimen/small_margin"
            android:orientation="vertical"
            android:layout_below="@+id/title">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/medium_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Beneficiary Name"/>

                <TextView
                    android:id="@+id/beneNameTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1.2"
                    android:gravity="right"
                    android:paddingRight="@dimen/small_margin"
                    android:text="12ab00"
                    android:textColor="@color/dark_blue_color"
                    android:textSize="@dimen/text_size_14sp"
                    />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Mobile No."/>
                <TextView
                    android:id="@+id/mobileTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="1111111"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Bank Name"/>
                <TextView
                    android:id="@+id/bankNameTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Bank"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Account No"/>
                <TextView
                    android:id="@+id/accountNoTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="100100"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Transaction Type"/>

                <TextView
                    android:id="@+id/txnTypeTv"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1.2"
                    android:gravity="right"
                    android:paddingRight="@dimen/small_margin"
                    android:text="priyanshi"
                    android:textColor="@color/dark_blue_color"
                    android:textSize="@dimen/text_size_14sp" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="RRN No"/>
                <TextView
                    android:id="@+id/rrnNoTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Remittance Amount"/>
                <TextView
                    android:id="@+id/remitAmountTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp"
                    android:text="Transaction Date"/>
                <TextView
                    android:id="@+id/txnDateTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/very_small_margin"
                android:layout_marginLeft="@dimen/small_margin"
                android:layout_marginRight="@dimen/small_margin"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView5"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="Agency Name"
                    android:textColor="@color/printDialogLableColor"
                    android:textSize="@dimen/text_size_14sp" />

                <TextView
                    android:id="@+id/agencyNameTv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="0.00"
                    android:gravity="right"
                    android:textSize="@dimen/text_size_14sp"
                    android:textColor="@color/dark_blue_color"
                    android:paddingRight="@dimen/small_margin"
                    android:layout_weight="1.2"/>

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/color_light_black"
                />

        </LinearLayout>

    </RelativeLayout>

    <LinearLayout
        android:id="@+id/buttonsLin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/large_margin"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingBottom="@dimen/small_margin"
        android:paddingRight="@dimen/medium_padding"
        android:layout_alignParentBottom="true"
        >

        <TextView
            android:id="@+id/print_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/small_margin"
            android:layout_marginRight="@dimen/small_margin"
            android:background="@drawable/blue_round_button_background"
            android:gravity="center"
            android:paddingBottom="@dimen/small_margin"
            android:paddingLeft="@dimen/large_margin"
            android:paddingRight="@dimen/large_margin"
            android:paddingTop="@dimen/small_margin"
            android:text="Back"
            android:visibility="visible"
            android:textColor="@color/color_white"
            android:textSize="@dimen/simple_content_text_size"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/delete_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/medium_margin"
            android:layout_marginRight="@dimen/small_margin"
            android:background="@color/app_blue_color"
            android:gravity="center"
            android:paddingBottom="@dimen/very_small_margin"
            android:paddingLeft="@dimen/large_margin"
            android:paddingRight="@dimen/large_margin"
            android:paddingTop="@dimen/very_small_margin"
            android:text="Resend Delete OTP"
            android:textColor="@color/color_white"
            android:visibility="gone" />


    </LinearLayout>


</RelativeLayout>