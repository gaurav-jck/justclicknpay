            android:orientation="vertical"
            android:layout_marginLeft="@dimen/margin_7dp"
            android:layout_marginRight="@dimen/margin_7dp"
            android:paddingBottom="@dimen/medium_margin">

            <LinearLayout
                android:id="@+id/home_lin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/large_margin"
                android:paddingLeft="@dimen/large_margin"
                android:paddingTop="@dimen/margin_7dp"
                android:paddingBottom="@dimen/margin_7dp"
                android:visibility="visible"
                android:orientation="horizontal"
                android:background="@drawable/white_round_button_background"
                >

                <ImageView
                    android:id="@+id/home_img"
                    android:layout_width="@dimen/drawer_image_size"
                    android:layout_height="@dimen/drawer_image_size"
                    app:srcCompat="@drawable/ic_home_black"
                    android:background="@drawable/solid_circle_drawer_background"/>

                <TextView
                    android:id="@+id/home_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/nav_home"
                    android:textSize="@dimen/drawer_content_text_size"
                    android:textColor="@color/main_text_color"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/drawer_text_left_margin"/>

                <ImageView
                    android:layout_width="@dimen/drawer_arrow_size"
                    android:layout_height="@dimen/drawer_arrow_size"
                    app:srcCompat="@drawable/arrow_left_back"
                    android:rotation="180"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/small_margin"
                    app:tint="@color/grey_dark" />

            </LinearLayout>

            <LinearLayout
