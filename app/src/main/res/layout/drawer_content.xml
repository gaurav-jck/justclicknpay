                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:text="@string/nav_balance"
                    android:textSize="@dimen/drawer_content_text_size"
                    android:textColor="@color/main_text_color"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="@dimen/drawer_text_left_margin"/>
                <ImageView
                    android:layout_width="@dimen/drawer_arrow_size"
                    android:layout_height="@dimen/drawer_arrow_size"
                    app:srcCompat="@drawable/arrow_left_back"
                    android:rotation="180"
                    android:layout_gravity="center_vertical"
                    android:layout_marginRight="@dimen/small_margin"
                    app:tint="@color/grey_dark" />
            </LinearLayout>

            <LinearLayout
