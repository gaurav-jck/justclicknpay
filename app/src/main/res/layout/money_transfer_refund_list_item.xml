<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/very_small_margin"
    android:background="@color/listItemBackground">
<LinearLayout
    android:id="@+id/linear"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Reservation ID"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp" />

        <TextView
            android:id="@+id/reservation_id_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Account"/>
        <TextView
            android:id="@+id/acount_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Mobile"/>
        <TextView
            android:id="@+id/mobile_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Update Date"/>
        <TextView
            android:id="@+id/update_date_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Transaction Type"/>
        <TextView
            android:id="@+id/transaction_type_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Transaction Amount"/>
        <TextView
            android:id="@+id/amount_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Service Charge"/>
        <TextView
            android:id="@+id/serviceChargeTv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Transcation ID"/>
        <TextView
            android:id="@+id/transaction_id_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Bank Ref no."/>
        <TextView
            android:id="@+id/bank_ref_no_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/transactionListLabelColor"
            android:textSize="@dimen/text_size_14sp"
            android:text="Status"/>
        <TextView
            android:id="@+id/statusTv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="0.00"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/dark_blue_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/very_small_margin"
        android:layout_marginLeft="@dimen/small_margin"
        android:layout_marginRight="@dimen/small_margin"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/listItemTextColorBlue"
            android:textSize="@dimen/text_size_14sp"
            android:visibility="invisible"
            android:text="Print"/>
        <TextView
            android:id="@+id/print_tv"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Print"
            android:layout_gravity="center"
            android:gravity="right"
            android:textSize="@dimen/text_size_14sp"
            android:textStyle="bold"
            android:textColor="@color/app_red_color"
            android:paddingRight="@dimen/medium_margin"
            android:layout_weight="0.3"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingRight="@dimen/medium_padding"
        android:paddingBottom="@dimen/small_margin"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/refund_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="@color/color_white"
            android:layout_marginLeft="@dimen/small_margin"
            android:layout_marginRight="@dimen/small_margin"
            android:background="@drawable/blue_round_button_background"
            android:paddingTop="@dimen/very_small_margin"
            android:paddingBottom="@dimen/very_small_margin"
            android:paddingLeft="@dimen/large_margin"
            android:paddingRight="@dimen/large_margin"
            android:text="Refund"/>



    </LinearLayout>

</LinearLayout>
</LinearLayout>